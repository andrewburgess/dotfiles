{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "style": "powerline",
          "template": "{{ if ne .Profile \"default\" }}\uf0ef  {{.Profile}}{{end}}",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0 ",
          "background": "#FF9300",
          "type": "aws"
        },
        {
          "style": "plain",
          "template": "{{ .HostName }} ",
          "foreground": "lightYellow",
          "background": "transparent",
          "type": "session"
        },
        {
          "properties": {
            "style": "folder"
          },
          "style": "plain",
          "template": "<#ffffff>in</> {{ .Path }} ",
          "foreground": "blue",
          "background": "transparent",
          "type": "path"
        },
        {
          "properties": {
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "powerline",
          "template": " {{ .UpstreamIcon }} {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Staging.Changed) (.Working.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \uf0c7 {{ .StashCount }}{{ end }}",
          "foreground": "darkGray",
          "powerline_symbol": "\ue0b0",
          "background": "green",
          "type": "git",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}yellow{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}red{{ end }}",
            "{{ if gt .Ahead 0 }}blue{{ end }}",
            "{{ if gt .Behind 0 }}cyan{{ end }}"
          ]
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "style": "austin",
            "threshold": 5000
          },
          "style": "powerline",
          "template": " <#fefefe>\u23f1\ufe0f </> <b>{{ .FormattedMs }}</b> ",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "background": "#5436fd",
          "type": "executiontime"
        },
        {
          "style": "plain",
          "template": " \uf64f {{ .CurrentDate | date .Format }} ",
          "foreground": "#963f1d",
          "background": "transparent",
          "type": "time"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "style": "plain",
          "template": "‚ùØ",
          "foreground": "lightGreen",
          "background": "transparent",
          "type": "text"
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "auto_upgrade": false,
  "final_space": true
}
